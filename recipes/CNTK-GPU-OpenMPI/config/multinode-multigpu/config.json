{
    "batch_shipyard": {
        "storage_account_settings": "<storage account specified in credentials.json>"
    },
    "global_resources": {
        "docker_images": [
            "alfpark/cntk:1.7.2-gpu-openmpi-refdata"
        ],
        "docker_volumes": {
            "shared_data_volumes": {
                "glustervol": {
                    "volume_driver": "glusterfs",
                    "container_path": "$AZ_BATCH_NODE_SHARED_DIR/gfs"
                }
            }
        }
    }
}
